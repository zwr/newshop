<h1>Listing product_packeds</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Description</th>
      <th>Url</th>
      <th>Quantity packed</th>
      <th>Quantity unpacked</th>
      <th>Price packed</th>
      <th>Price unpacked</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_packeds.each do |product_packed| %>
      <tr>
        <td><%= product_packed.id %></td>
        <td><%= product_packed.name %></td>
        <td><%= product_packed.description %></td>
        <td><%= product_packed.url %></td>
        <td><%= product_packed.quantity_packed %></td>
        <td><%= product_packed.quantity_unpacked %></td>
        <td><%= product_packed.price_packed %></td>
        <td><%= product_packed.price_unpacked %></td>
        <td><%= link_to 'Show', product_packed %></td>
        <td><%= link_to 'Edit', edit_product_packed_path(product_packed) %></td>
        <td><%= link_to 'Destroy', product_packed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product packed', new_product_packed_path %>
